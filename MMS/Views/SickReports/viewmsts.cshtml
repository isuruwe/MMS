<script src="~/Scripts/js/smoothscroll.js"></script>
<script src="~/Scripts/js/isotope.js"></script>
<script src="~/Scripts/js/imagesloaded.min.js"></script>
<script src="~/Scripts/js/nivo-lightbox.min.js"></script>
<script src="~/Scripts/js/jquery.backstretch.min.js"></script>
<script src="~/Scripts/js/wow.min.js"></script>
<script src="~/Scripts/js/custom.js"></script>

<script>
    $(document).ready(function () {

        document.getElementById('datest').valueAsDate = new Date();

    });

    function loadold() {


        angular.element(document.getElementById('epicontrol')).scope().Getclaim();



 }</script>

<div ng-app="EpisodeApp" class="container">

    <div ng-controller="EpisodeCntrl" id="epicontrol" ng-init="">

        <div ng-show="divPor" class="form-horizontal">

            <section class="navbar navbar-fixed-top custom-navbar" role="navigation">
                <div class="container">

                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-left">
                            <li>
                                <a href="@Url.Action("Index","Home")">HOME</a>
                            </li>
                            


                        </ul>
                        <ul class="nav navbar-nav navbar-right">
                          


                            @*<li><a href="#about" class="smoothScroll">ABOUT</a></li>*@

                            @*<li><a href="#portfolio" class="smoothScroll">PORTFOLIO</a></li>*@

                        </ul>

                    </div>




                </div>
            </section>
            <div ng-show="showloader" id="loading">
                <img id="loading-image" src="~/Content/Images/loading.png" />
            </div>
            <section id="team">
                <div class="container">
                    <div class="row">
                        <script>
                            $(document).ready(function () {

                                document.getElementById('datest').valueAsDate = new Date();
                            });

                        </script>
                        <div class="col-md-12 col-sm-12">
                            <div class="section-title">

                                <h1 class="heading bold">Medical Examiation</h1>
                                <hr>
                            </div>
                        </div>

                     



                  

                    </div>
                </div>
            </section>

            <section id="work">
                <div class="container">
                    <div class="row">




                      


                        <div class="col-lg-12 ">
                            <div class="panel panel-primary">
                                <div class="panel-heading">View Medical Examination Status</div>
                                <div class="panel-body">
                                    <div class="form-group">
                                        <table class="table table-striped table-condensed">
                                            <tbody>
                                                <tr>
                                                    <td>Service No</td>
                                                    <td>
                                                        <input id="svcid" type="text" class="form-control" onchange="" style="color:blue" ng-model="svcid" />
                                                    </td>
                                                </tr>
                                                <tr><td>Session</td><td><select id="sess1" ng-model="sess1">
    <option value="1">1</option>
    <option value="2">2</option>
    <option value="3">3</option>
    <option value="4">Special-1</option>
    <option value="5">Special-2</option>
    <option value="6">Special-3</option>

</select></td></tr>
<tr>
                                                    
                                                    <td>Year</td>
                                                    <td>
                                                        <input type="text" ng-model="yyr" class="form-control">
                                                        
                                                    </td>
                                                </tr>
                                                <tr>

                                                    <td>

                                                        <input type="button" ng-click="viewmsts(svcid,yyr,sess1)" value="View" />
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>Service No,Rank Name</td>
<td>
                                                        {{msdata[0].sno}}
                                                       {{msdata[0].rnkname}}
                                                        {{msdata[0].inililes}} {{msdata[0].sname}}
                                                    </td>
                                                </tr>
                                                <tr>
                                                    <td>Medical status</td>
                                                    <td>
                                                       
                                                        <div ng-if="(msdata[0].sv=='1')"> Medical Fit </div>
                                                        <div ng-if="(msdata[0].sv=='2')"> Medical Un Fit </div>
                                                    </td>
                                                </tr>

                                                <tr>
                                                    <td>Remarks</td>
                                                    <td>
                                                        {{msdata[0].pdids}}
                                                    </td>
                                                </tr>

                                                <tr>
                                                    <td>MES</td>
                                                    <td>
                                                        {{msdata[0].locdet}}
                                                    </td>
                                                </tr>

                                                <tr>
                                                    <td>Date</td>
                                                    <td>
                                                        {{msdata[0].crdate |limitTo:-15 |limitTo:13 | date:'dd/MM/yyyy'}}
                                                    </td>
                                                </tr>

                                                
                                            </tbody>
                                        </table>
                                        <table class="table table-striped">

                                            <tr ng-repeat="clitem in clitems" class="item">
                                                <td width="50%"><b>{{clitem.cnbenef}}</b> </td>
                                                <td width="50%"><b>{{clitem.cclmamn}}</b> </td>
                                                <td width="50%"><b>{{clitem.cevntdt|limitTo:-15 |limitTo:13 | date:'dd/MM/yyyy'}}</b> </td>

                                                <td><button ng-click="clremoveItem($index)">Remove</button></td>
                                            </tr>
                                        </table>

                                    </div>
                                </div>
                            </div>
                        </div>


                    </div>
                   
                </div>
            </section>


        </div>
    </div>
</div>


<div>
    @Html.ActionLink("Back to List", "Index")
</div>


