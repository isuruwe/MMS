                                    <div class="form-group">
                                        @Html.Label("Build:", htmlAttributes: new { @class = "control-label col-md-2" })
                                        <div class="col-md-10">
                                            <label ng-style="c6">Lean</label><input type="checkbox" ng-model="Lean" ng-change="changeColor6(Lean);" /><label ng-style="c7"> Average</label><input type="checkbox" ng-model="Average" ng-change="changeColor7(Average);" /> <label ng-style="c8"> Obese</label> <input type="checkbox" ng-model="Obese" ng-change="changeColor8(Obese);" />
                                        </div>
                                    </div>
                                    <hr />
                                    <div class="form-group">
                                        @*@Html.Label("Location", htmlAttributes: new { @class = "control-label col-md-2" })*@
