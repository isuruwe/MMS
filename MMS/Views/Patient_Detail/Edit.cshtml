                                <hr>
                            </div>
                        </div>


                        @*General Examination*@
                        <div class="col-lg-4 ">

                            <div class="panel panel-primary">
                                <div class="panel-heading">
                                    General Examination <input type="checkbox" ng-model="ge" />
                                </div>
                                <div class="panel-body" ng-show="ge">
