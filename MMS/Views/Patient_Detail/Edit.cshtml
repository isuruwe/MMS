                                        <div class="col-lg-12" style="padding-left:0px;"> @*@Html.Label("Alert", htmlAttributes: new { @class = "control-label col-md-2" })*@ </div>
                                        <div class="col-md-12">
                                            <label ng-style="c1">Alert</label> <input type="checkbox" ng-model="Alert" ng-change="changeColor1(Alert);" /> <label ng-style="c2">Confused </label><input type="checkbox" ng-model="Confused" ng-change="changeColor2(Confused);" /> <label ng-style="c3">Drowsy</label> <input type="checkbox" ng-model="Drowsy" ng-change="changeColor3(Drowsy);" /> <label ng-style="c4">Unconscious</label> <input type="checkbox" ng-model="Unconscious" ng-change="changeColor4(Unconscious);" />
                                        </div>
                                    </div>

                                    <div class="form-group">
                                        @Html.Label("Pain ", htmlAttributes: new { @class = "control-label col-md-2" })
