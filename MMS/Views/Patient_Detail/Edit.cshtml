                                        </div>
                                    </div>
                                    <hr />
                                    <div class="form-group">
                                        @*@Html.Label("Location", htmlAttributes: new { @class = "control-label col-md-2" })*@
                                        <div class="col-md-12">

                                            <label ng-style="c11"> Pallor</label><input type="checkbox" ng-model="Pallor" ng-change="changeColor11(Pallor);" /> <label ng-style="c12">Icterus</label><input type="checkbox" ng-model="Icterus" ng-change="changeColor12(Icterus);" /> <label ng-style="c13">Arcus</label>  <input type="checkbox" ng-model="Arcus" ng-change="changeColor13(Arcus);" /> <label ng-style="c14">Xanthomata</label> <input type="checkbox" ng-model="Xanthomata" ng-change="changeColor14(Xanthomata);" />
