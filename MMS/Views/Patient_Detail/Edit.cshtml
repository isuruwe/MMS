                                                <input type="button" ng-click="addItem(VTID.VTID,vvalue)" value="Add" />
                                            </div>
                                        </div>
                                        <table class="table table-striped">

                                            <tr ng-repeat="item in items" class="item">
                                                <td><b>{{item.name}}</b> </td>
                                                <td><b>:</b>{{item.amount}} </td>
