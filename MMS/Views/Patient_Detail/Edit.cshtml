                                        <div class="form-group">
                                            @Html.Label("Type", htmlAttributes: new { @class = "control-label col-md-2" })
                                            <div class="col-md-10">
                                                <select id="VTID" ng-model="VTID" ng-options="c as c.VitalType for c in VitalTypes">
                                                    <option value="">-- Select Vital Type --</option>
                                                </select>
                                            </div>
                                        </div>

